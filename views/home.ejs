<%- include('partials/head') %>

<div class="levels-list">
  <% for (let level of queryData) { %>
    <div class="level">
      <div class="level-top">
        <img class="style-image" src="<%= styleImages[level.style] %>" alt="level.style">
      <span class="level-title"><%= level.level_name %> </span>
      </div>
      <div class="level-bottom">
        <div class="bottom-left">
          <div class="thumbnail">[THUMB]</div>
          <div class="upload-date">
            <span><%= level.upload_date_day %> </span>
            <span><%= level.upload_date_time %> </span>
          </div>
        </div>
        <div class="bottom-right">
          <p class="likes-feet">
            <span class="likes">[L] <%= level.likes %> </span>
            <span class="footprints">[F] <%= level.footprints %></span>
          </p>
          <p class="tags">
            <span class="tag1">[T] <%= level.tag1 %></span>
            <% if (level.tag2 != null) { %>
            <span class="tag2">[T] <%= level.tag2 %></span>
            <% } %>
          </p>
        </div>
      </div>
    </div>
  <% } %>
</div>

<%- include('partials/foot')  %>
